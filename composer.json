{
    "name": "damis/damis-gui",
    "license": "MIT",
    "type": "project",
    "description": "The DAMIS GUI application, upgraded to Symfony 6.4.",
    "require": {
        "php": ">=8.4",
        "apy/datagrid-bundle": "^7.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "*",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "friendsofsymfony/jsrouting-bundle": "^3.6",
        "friendsofsymfony/user-bundle": "^4.0",
        "guzzlehttp/guzzle": "^7.4",
        "jms/serializer-bundle": "^5.3",
        "knplabs/knp-menu-bundle": "^3.4",
        "knplabs/knp-paginator-bundle": "^6.3",
        "phpoffice/phpspreadsheet": "^1.25",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/flex": "^2.0",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bridge": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^3.4",
        "twig/twig": "^3.4",
        "willdurand/js-translation-bundle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.58",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Base\\": "src/Base/",
            "Damis\\": "src/Damis/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
