{% block body %}
    <div class="col-lg-12">
        <form method="POST">
            <input type="hidden" name="params" id="params" value="{{ response }}" />
            <div class="form-group">
                {{ form_label(form.retFilteredData) }}
                {{ form_widget(form.retFilteredData) }}
                {{ form_errors(form.retFilteredData) }}
            </div>
            <div class="form-group">
                {{ form_label(form.zValue) }}
                {{ form_widget(form.zValue) }}
                {{ form_errors(form.zValue) }}
            </div>
            <div class="form-group">
                {{ form_label(form.attrIndex) }}
                {{ form_widget(form.attrIndex) }}
                {{ form_errors(form.attrIndex) }}
            </div>
            <div class="col-md-12">
                {{ "Fields marked with <span style='color:red;'>*</span> are mandatory"|trans({}, 'ExperimentBundle')|raw }}
            </div>
            {{ form_widget(form._token) }}
        </form>
    </div>
{% endblock %}