<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="{{ app.request.locale }}" lang="{{ app.request.locale }}">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
    <meta http-equiv="content-language" content="{{ app.request.locale }}"/>

    <title>{% block title %}{{ project }}{% endblock %}</title>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico?v=140305') }}" />

    {% block stylesheets %}
        {% if app.environment == "prod" %}
            <link href="{{ asset('/css/smoothness/jquery-ui-1.10.4.custom.min.css') }}" rel="stylesheet"/>
            <link href="{{ asset('/css/bootstrap.min.css') }}" rel="stylesheet"/>
            <link href="{{ asset('/css/bootstrap-theme.min.css') }}" rel="stylesheet"/>
        {% else %}
            <link href="{{ asset('/css/smoothness/jquery-ui-1.10.4.custom.css') }}" rel="stylesheet"/>
            <link href="{{ asset('/css/bootstrap.css') }}" rel="stylesheet"/>
            <link href="{{ asset('/css/bootstrap-theme.css') }}" rel="stylesheet"/>
        {% endif %}
            <link href="{{ asset('/css/styles.css?v=140305') }}" rel="stylesheet" />
            <link href="{{ asset('/css/fileType.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/jquery.dataTables.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/grid-style.css?v=140312') }}" rel="stylesheet" />
    {% endblock %}
</head>

<body>
    <div id="wrap">

        <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ path('base_main_default_index') }}">{{ project }}</a>
                </div>
                <div class="navbar-collapse collapse">
                    {% include "BaseMainBundle::topMenu.html.twig" %}
                </div>
            </div>
        </div>

        <div class="container">
            {% block body %}
            {% endblock %}
        </div>

        <div id="push"></div>

    </div>

    <div id="footer">
        <div class="container container-footer">
            <p class="text-muted">
                <a href="http://www.inscience.lt" target="_blank">InScience.LT </a>
                <a href="https://github.com/InScience/DAMIS-GUI" target="_blank">Source code</a>
                <a href="https://www.gnu.org/licenses/agpl.txt" rel="license" class="helptext" target="_blank"> (aGPL) </a>
            </p>
        </div>
    </div>

    {% block javascripts %}
        {% if app.environment == "prod" %}
            <script src="{{ asset('/js/jquery-1.11.0.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery-ui-1.10.4.custom.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.ui.touch-punch.min.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/typeahead.bundle.min.js?v=0.10.1') }}" type="text/javascript"></script>

            <script src="{{ asset('js/flot/jquery.flot.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/flot/jquery.flot.symbol.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/jquery.jsPlumb-1.5.5-min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.formset.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/spin.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.spin.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/colpick.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.dataTables.min.js') }}" type="text/javascript"></script>
        {% else %}
            <script src="{{ asset('/js/jquery-1.11.0.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery-ui-1.10.4.custom.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.ui.touch-punch.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/typeahead.bundle.js?v=0.10.1') }}" type="text/javascript"></script>

            <script src="{{ asset('js/flot/jquery.flot.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/flot/jquery.flot.symbol.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/bootstrap.js') }}" type="text/javascript"></script>

            <script src="{{ asset('js/jquery.jsPlumb-1.5.5.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.formset.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/spin.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.spin.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/colpick.js') }}" type="text/javascript"></script>
            <script src="{{ asset('js/jquery.dataTables.min.js') }}" type="text/javascript"></script>
        {% endif %}
        <script src="{{ asset('/js/fileupload.js') }}" type="text/javascript"></script>
        <script src="{{ asset('/js/filename.js') }}" type="text/javascript"></script>
        <script src="{{ asset('js/tinymce/tinymce.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
        <script src="{{ asset('js/hogan-2.0.0.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}" type="text/javascript"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}" type="text/javascript"></script>
    {% endblock %}

</body>

</html>
